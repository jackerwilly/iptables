sudo iptables -A INPUT -p tcp --dport 22 -j ACCEPT		accept tcp over sourse port 22 INTO network
sudo iptables -A OUTPUT -p tcp --sport 22 -j ACCEPT		same thing but OUT of network

sudo iptables -A INPUT -p tcp --dport 23 -j ACCEPT		same thing but telnet
sudo iptables -A OUTPUT -p tcp --sport 23 -j ACCEPT		^

sudo iptables -A INPUT -p tcp --dport 3389 -j ACCEPT		same but rdp
sudo iptables -A OUTPUT -p tcp --sport 3389 -j ACCEPT	^

sudo iptables -P INPUT DROP					changes default policy to drop
sudo iptables -P OUTPUT DROP
sudo iptables -P FORWARD DROP

sudo iptables -A INPUT -s 10.10.0.40 -p icmp --icmp-type echo-request -j ACCEPT
sudo iptables -A INPUT -s 10.10.0.40 -p icmp --icmp-type echo-reply -j ACCEPT
sudo iptables -A OUTPUT -d 10.10.0.40 -p icmp --icmp-type echo-request -j ACCEPT
sudo iptables -A OUTPUT -d 10.10.0.40 -p icmp --icmp-type echo-reply -j ACCEPT

sudo iptables -A INPUT -p tcp --dport 6579 -j ACCEPT
sudo iptables -A INPUT -p udp --dport 6579 -j ACCEPT
sudo iptables -A INPUT -p tcp --dport 4444 -j ACCEPT
sudo iptables -A INPUT -p udp --dport 4444 -j ACCEPT
sudo iptables -A OUTPUT -p tcp --sport 6579 -j ACCEPT
sudo iptables -A OUTPUT -p udp --sport 6579 -j ACCEPT
sudo iptables -A OUTPUT -p tcp --sport 4444 -j ACCEPT
sudo iptables -A OUTPUT -p udp --sport 4444 -j ACCEPT

sudo iptables -A OUTPUT -p tcp --dport 22 -j ACCEPT
sudo iptables -A INPUT -p tcp --sport 22 -j ACCEPT
